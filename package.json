{
  "name": "postit",
  "version": "0.0.1",
  "description": "an app that allows creation of groups for notifications",
  "main": "server/server.js",
  "scripts": {
    "dev": "webpack-dev-server --content-base client/src --inline --hot",
    "client:test": "jest --coverage client/tests",
    "server-pre-test": "nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/tests/**/*.js --timeout 10000",
    "clean-db": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "clean-db-prod": "NODE_ENV=production sequelize db:migrate:undo:all && NODE_ENV=production sequelize db:migrate",
    "setup-test": "NODE_ENV=test npm run clean-db && NODE_ENV=test sequelize db:seed:all",
    "server:test": "npm run setup-test && npm run server-pre-test",
    "start": "NODE_ENV=production webpack -p --config webpack.config.prod.js && babel-node -- server/server.js",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node -- server/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "e2e-setup": "selenium-standalone install",
    "e2e:test": "./node_modules/.bin/nightwatch",
    "build": "webpack -p --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noordean/PostIt.git"
  },
  "author": "noordean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noordean/PostIt/issues"
  },
  "homepage": "https://github.com/noordean/PostIt#readme",
